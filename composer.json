{
    "name": "home/libraree-bundle",
    "version": "1.0.0",
    "type": "contao-bundle",
    "description": "Adds Libraree to Contao 4",
    "keywords":["contao","libraree"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "HolsteinMedia",
            "homepage": "http://www.holsteinmedia.com"
        }
    ],
    "require": {
        "php": "^5.6",
        "contao/core-bundle": "^4.4.2"
    },
    "autoload": {
        "psr-4": {
            "Home\\LibrareeBundle\\": ""
        },
        "classmap": [
            "Resources/contao/"
        ],
        "exclude-from-classmap": [
            "Resources/contao/config/",
            "Resources/contao/templates/"
        ]
    },
    "extra": {
      "contao-manager-plugin": "Home\\LibrareeBundle\\ContaoManager\\Plugin"
    }
}
